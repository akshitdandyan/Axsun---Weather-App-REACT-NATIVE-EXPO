{
  "expo": {
    "name": "Axsun",
    "slug": "Axsun",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#002651"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#002651"
      },
      "package": "com.eraaxit.axitfirstreactnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
